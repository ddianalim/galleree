<div>
  <!-- <form action="/users" method="post"> -->

  <form action="http://gallereewebapp.s3.amazonaws.com/" method="post" enctype="multipart/form-data">

    <legend>New User</legend>

    <div class="form-group">
      <label for="user-username">Username</label>
      <input type="text" name="username" class="form-control" id="user-username" placeholder="Username">
    </div>

    <div class="form-group">
      <label for="user-password">Password</label>
      <input type="text" name="password" class="form-control" id="user-password" placeholder="Password">
    </div>

    <div class="text-center">
      <button type="submit" class="btn btn-primary">Submit</button>
    </div>
  </form>
</div>
